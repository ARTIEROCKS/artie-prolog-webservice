{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "name": "Debug NestJS",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run-script", "start:debug"],
        "port": 9229,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "sourceMaps": true,
        "restart": true,
        "protocol": "inspector"
      }
    ]
  }
  